# API key for the embeddings model. Defaults to OpenAI embeddings
OPENAI_API_KEY=

# PostgreSQL configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=langconnect_dev

# CORS configuration. Must be a JSON array of strings
ALLOW_ORIGINS=["http://localhost:3000"]


# For authentication
SUPABASE_URL=
# This must be the service role key
SUPABASE_KEY=

# Подключение к гигачату
LANGCONNECT_EMBEDDINGS=gigachat:EmbeddingsGigaR
GIGACHAT_CREDENTIALS=
GIGACHAT_SCOPE=
GIGACHAT_VERIFY_SSL_CERTS=0

# Секретный ключ для доступа в обход supabase
SECRET_TOKEN=